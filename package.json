{
   "name": "contentagen-nx",
   "version": "0.0.1",
   "devDependencies": {
      "@biomejs/biome": "2.1.3",
      "@commitlint/cli": "^19.8.1",
      "@commitlint/config-conventional": "^19.8.1",
      "@nx/js": "21.1.2",
      "@swc-node/register": "~1.9.1",
      "@swc/core": "~1.5.7",
      "@swc/helpers": "~0.5.11",
      "@tooling/typescript": "workspace:*",
      "husky": "^9.1.7",
      "msw": "^2.10.4",
      "msw-trpc": "^2.0.1",
      "nx": "21.1.2",
      "typescript": "catalog:development"
   },
   "engines": {
      "node": ">=20"
   },
   "private": true,
   "scripts": {
      "build:all": "nx run-many -t build --parallel=3",
      "db:studio": "nx run database:studio",
      "db:push": "nx run database:push",
      "auth:generate": "nx run authentication:auth-schema-generate",
      "check": "nx run-many -t check --parallel=3",
      "dev:all": "nx run-many -t dev --parallel=5",
      "dev:content-apps": "nx run-many -t dev --parallel=3 -p  landing-page blog docs",
      "dev:landing-page": "nx run-many -t dev --parallel=1 -p  landing-page ",
      "dev:blog": "nx run-many -t dev --parallel=1 -p  blog ",
      "dev:docs": "nx run-many -t dev --parallel=1 -p  docs",
      "dev:dashboard": "nx run-many -t dev --parallel=3 -p server dashboard tasks",
      "dev:server": "nx run-many -t dev --parallel=1 -p server ",
      "dev:dashboard-only": "nx run-many -t dev --parallel=1 -p dashboard",
      "format": "biome check --write .",
      "typecheck": "nx run-many -t typecheck --parallel=3",
      "test": "nx run-many -t test --parallel=3",
      "release": "nx release"
   },
   "trustedDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "protobufjs"
   ],
   "workspaces": {
      "catalogs": {
         "payment": {
            "@polar-sh/sdk": "^0.34.5"
         },
         "enviroment": {
            "dotenv-cli": "8.0.0",
            "shx": "0.4.0"
         },
         "trpc": {
            "@trpc/client": "^11.4.3",
            "@trpc/tanstack-react-query": "^11.4.3",
            "@trpc/server": "^11.4.3"
         },
         "testing": {
            "msw": "^2.10.4",
            "msw-trpc": "^2.0.1"
         },
         "astro": {
            "@astrojs/ts-plugin": "^1.10.4",
            "@astrojs/check": "^0.9.4",
            "@astrojs/node": "^9.2.2",
            "@astrojs/react": "^4.3.0",
            "@astrojs/sitemap": "^3.4.1",
            "@lucide/astro": "^0.511.0",
            "astro": "5.10.0",
            "@arcjet/astro": "^1.0.0-beta.8"
         },
         "tanstack": {
            "@tanstack/react-router": "^1.129.8",
            "@tanstack/router-plugin": "^1.129.8",
            "@tanstack/react-query": "^5.66.5",
            "@tanstack/react-router-with-query": "^1.114.3",
            "@tanstack/react-query-devtools": "^5.66.5"
         },
         "vite": {
            "vite-tsconfig-paths": "^5.1.4",
            "@vitejs/plugin-react": "^4.3.4",
            "@tailwindcss/vite": "^4.1.8",
            "vite": "^6.1.0",
            "vitest": "^3.0.5"
         },
         "database": {
            "drizzle-orm": "^0.44.2",
            "drizzle-typebox": "^0.3.3",
            "@sinclair/typebox": "^0.34.35",
            "pg": "^8.16.3",
            "@types/pg": "^8.15.4",
            "drizzle-kit": "^0.31.4"
         },
         "auth": {
            "better-auth": "1.3.0",
            "@polar-sh/better-auth": "^1.0.6"
         },
         "development": {
            "typescript": "5.8.3"
         },
         "react": {
            "@types/react": "^19.1.6",
            "@types/react-dom": "^19.1.5",
            "react": "^19.1.0",
            "react-dom": "^19.1.0"
         },
         "server": {
            "compression": "^1.7.5",
            "express": "^4.21.2",
            "get-port": "^7.1.0",
            "isbot": "^5.1.22",
            "node-fetch": "^3.3.2",
            "elysia": "1.3.6"
         },
         "ui": {
            "@tailwindcss/vite": "^4.1.8",
            "tailwindcss": "^4.0.8",
            "sonner": "^2.0.5"
         },
         "transactional": {}
      },
      "packages": [
         "tooling/*",
         "packages/*",
         "apps/*"
      ]
   },
   "dependencies": {
      "@openrouter/ai-sdk-provider": "^0.7.3",
      "@sinclair/typebox": "^0.34.38",
      "@tiptap/pm": "^3.0.7",
      "@tiptap/react": "^3.0.7",
      "@tiptap/starter-kit": "^3.0.7",
      "ajv": "^8.17.1",
      "ajv-formats": "^3.0.1"
   }
