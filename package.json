{
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tooling/typescript": "workspace:*",
    "husky": "^9.1.7",
    "nx": "21.1.2",
    "typescript": "catalog:development"
  },
  "engines": {
    "node": ">=20"
  },
  "name": "elysiajs-monorepo",
  "private": true,
  "scripts": {
    "build:all": "nx run-many -t build --parallel=5",
    "check": "nx run-many -t check --parallel=5",
    "dev:all": "nx run-many -t dev --parallel=5",
    "dev:content-apps": "nx run-many -t dev --parallel=5 -p  landing-page blog docs",
    "dev:dashboard": "nx run-many -t dev --parallel=5 -p server dashboard",
    "format": "biome check --write .",
    "typecheck": "nx run-many -t typecheck --parallel=5"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "protobufjs"
  ],
  "version": "0.0.1",
  "workspaces": {
    "catalogs": {
      "astro": {
        "@astrojs/check": "^0.9.4",
        "@astrojs/node": "^9.2.2",
        "@astrojs/react": "^4.3.0",
        "@astrojs/sitemap": "^3.4.1",
        "@lucide/astro": "^0.511.0",
        "astro": "5.10.0"
      },
      "auth": {
        "better-auth": "^1.2.8",
        "@polar-sh/better-auth": "^1.0.3"
      },
      "development": {
        "typescript": "5.8.3"
      },
      "react": {
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.5",
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
      },
      "server": {
        "elysia": "1.3.5"
      },
      "ui": {
        "@tailwindcss/vite": "^4.1.8",
        "sonner": "^2.0.5"
      }
    },
    "packages": [
      "tooling/*",
      "packages/*",
      "apps/*"
    ]
  },
  "dependencies": {
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0"
  }
}
