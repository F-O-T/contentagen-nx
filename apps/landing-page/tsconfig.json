{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@api": [
        "../../apps/server/src/index"
      ],
      "@/*": [
        "./src/*"
      ],
      "@api/*": [
        "../../apps/server/src/*"
      ],
      "@packages/ui/*": [
        "../../packages/ui/src/*"
      ]
    }
  },
  "exclude": [
    "dist"
  ],
  "extends": "@tooling/typescript/astro.json",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ]
}
